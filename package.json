{
  "name": "usof-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.1.2",
    "concurrently": "^7.4.0",
    "eslint": "^8.25.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev:server": "nodemon -r dotenv/config ../usof-backend/server.js dotenv_config_path=../usof-backend/.env",
    "dev:client": "npm start",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\""
  },
  "nodemonConfig": {
    "ignore": [
      "../usof-backend"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
